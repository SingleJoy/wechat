<!--pages/home/home.wxml-->
<view class="container">

   <view class="content">
       <view class="content-list">
           <view class="title-name">{{homeTitle}}</view>
           <view class="app-list">
               <button class="black"  bindtap="calculator">计算器</button>
               <button class="green"  bindtap="weather">看天气</button>
               <button class="blue"  bindtap="travel">旅行表单</button>
               <button class="red"  bindtap="travel">听音乐</button>
           </view>
       </view>

        <view class="content-list">
            <text class="title-name">其他:</text>
            <view></view>
        </view>

    </view>

</view>
